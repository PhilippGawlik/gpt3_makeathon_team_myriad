                    "propvalue":                     {
                        "@col-index": "2",
                        "p":                         {
                            "@context": "Cond",
                            "$":                             [
                                "Liquid detergent works faster and is recommended for shorter",
                                {"link":                                 {
                                    "@BSH-Linktype": "Kurzlink",
                                    "@source-id": "63050396238475403-9",
                                    "@target-id": "135107988968018187-9",
                                    "@use": "grab",
