    "title": {"$": "Arrange tableware"},
    "shortdesc": {"$": "Arrange the tableware correctly to optimise the dishwashing result and prevent damage to both the appliance and tableware."},
    "hint":     {
        "@hint-type": "multi",
        "@type": "user-oriented",
        "b": {"$": "Tips:"},
        "BSHCollectionOfElements":         [
            {"sub-hint": {"p":             {
                "@context": "hint",
                "$": "Using the appliance allows you to save energy and water compared to washing-up by hand."
            }}},
            {"sub-hint": {"p":             {
                "@context": "hint",
                "$": "See our website for examples of how to load your appliance efficiently, free of charge."
            }}},
            {"sub-hint": {"p":             {
                "@context": "hint",
                "$": "To save energy and water, load the machine with the number of place settings specified.",
                "link":                 {
                    "@BSH-Linktype": "Langlink",
                    "@source-id": "36028797166086283-9",
