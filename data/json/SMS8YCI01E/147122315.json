    "instructions":     {
        "BSHCollectionOfElements":         [
            {"instruction":             {
                "p":                 {
                    "@context": "instructions",
                    "$": "Remove large remnants of food from your tableware."
                },
                "instruction-info": {"p":                 {
                    "@context": "instructions",
                    "$": "To save resources, do not pre-rinse tableware under running water."
                }}
            }},
            {"instruction":             {
                "p":                 {
                    "@context": "instructions",
                    "$": "Observe the following when arranging tableware:"
                },
                "instruction-info": {"ul": {"BSHCollectionOfElements":                 [
                    {"li":                     {
                        "@class": "TextModule2",
                        "@base-id": "2128174859",
                        "@chapternumbering": "yes",
                        "@showintoc": "yes",
                        "@linkid": "82B45CF0-6B30-4C5C-AFAB-27E35CCEE728",
                        "@fragment-element": "li",
                        "@aspect-name": "en",
                        "@specific-culture": "en-GB",
                        "@ref": "2128174859",
                        "@id": "2128174859-9",
                        "@context": "instructions",
                        "p":                         {
                            "@context": "li",
                            "$": "Put heavily soiled tableware in the bottom basket, e.g. pans. The more powerful the spray jet, the better the dishwashing result will be."
                        }
                    }},
                    {"li":                     {
                        "@context": "instructions",
                        "p":                         {
                            "@context": "li",
                            "$": "To prevent damage to tableware, arrange it so it is stable and cannot tip over."
                        }
                    }},
                    {"li":                     {
                        "@context": "instructions",
                        "p":                         {
                            "@context": "li",
                            "$": "To prevent injury, arrange cutlery with the points and sharp edges downwards."
                        }
                    }},
                    {"li":                     {
                        "@context": "instructions",
                        "p":                         {
                            "@context": "li",
                            "$": "Position containers with the openings facing down so no water can collect in them."
                        }
                    }},
                    {"li":                     {
                        "@class": "TextModule2",
                        "@base-id": "2128322699",
                        "@chapternumbering": "yes",
                        "@showintoc": "yes",
                        "@linkid": "37615E60-2817-47C7-B329-26FF502C772B",
                        "@fragment-element": "li",
                        "@aspect-name": "en",
                        "@specific-culture": "en-GB",
                        "@ref": "2128322699",
                        "@id": "2128322699-9",
                        "@context": "instructions",
                        "p":                         {
                            "@context": "li",
                            "$": "Do not block the spray arms - make sure that they can turn freely."
                        }
                    }},
                    {"li":                     {
                        "@class": "TextModule2",
                        "@base-id": "2128377995",
                        "@chapternumbering": "yes",
                        "@showintoc": "yes",
                        "@linkid": "3334103C-4A71-46ED-AEBA-9A0EA695D65D",
                        "@fragment-element": "li",
                        "@aspect-name": "en",
                        "@specific-culture": "en-GB",
                        "@ref": "2128377995",
                        "@id": "2128377995-9",
                        "@context": "instructions",
                        "p":                         {
                            "@context": "li",
                            "$": "Do not place small parts in the tablet collecting tray and do not block it with tableware so as not to obstruct the lid of the detergent dispenser."
                        }
                    }}
                ]}}
            }}
        ],
