        "b": {"$": "Notes:"},
        "BSHCollectionOfElements":         [
            {"sub-hint": {"p":             {
                "@context": "hint",
                "BSHCollectionOfElements":                 [
                    {"$": "Run the"},
                    {"variable":                     {
                        "@linkid": "4718860172",
                        "@name": "3325",
                        "@detailed-topic-type": "Non-specific",
                        "@variableType": "Standard",
                        "@variable": "Machine Care",
                        "@variableName": "Machine Care 0",
                        "@variant": "1001",
                        "@variantName": "Index",
                        "$": "Machine Care"
                    }},
                    {"$": "programme without any tableware in the dishwasher."}
                ]
            }}},
            {"sub-hint": {"p":             {
                "@context": "hint",
                "$": "Only use machine care products and machine cleaners specially designed for dishwashers."
            }}},
            {"sub-hint": {"p":             {
                "@context": "hint",
                "$": "Make sure that there are no aluminium parts, e.g. extractor hood grease filters or aluminium pans, in the interior of the appliance."
            }}},
            {"sub-hint": {"p":             {
                "@context": "hint",
                "BSHCollectionOfElements":                 [
                    {"$": "If you have not run"},
                    {"variable":                     {
                        "@linkid": "4718102924",
                        "@name": "3325",
                        "@detailed-topic-type": "Non-specific",
                        "@variableType": "Standard",
                        "@variable": "Machine Care",
                        "@variableName": "Machine Care 0",
                        "@variant": "1001",
                        "@variantName": "Index",
                        "$": "Machine Care"
                    }},
                    {"$": "after 3 washing cycles, the indicator for"},
                    {"variable":                     {
                        "@linkid": "4718102924",
                        "@name": "3325",
                        "@detailed-topic-type": "Non-specific",
                        "@variableType": "Standard",
                        "@variable": "Machine Care",
                        "@variableName": "Machine Care 0",
                        "@variant": "1001",
                        "@variantName": "Index",
                        "$": "Machine Care"
                    }},
                    {"$": "will go out automatically."}
                ]
            }}},
            {"sub-hint": {"p":             {
                "@context": "hint",
                "$": "For optimum cleaning performance, ensure the correct placement of cleaners."
            }}},
            {"sub-hint": {"p":             {
                "@context": "hint",
                "$": "Follow the safety instructions given on the packagings of the machine care products and machine cleaners."
            }}},
            {"sub-hint": {"p":             {
                "@context": "hint",
                "BSHCollectionOfElements":                 [
                    {"$": "Activate the"},
                    {"link":                     {
                        "@BSH-Linktype": "Kurzlink",
                        "@source-id": "4721161867-9",
                        "@target-id": "27021598340680459-9",
                        "@use": "grab",
